<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>sgp</display-name>
	<servlet>
		<servlet-name>ListerCollaborateursControlle</servlet-name>
		<servlet-class>dev.sgp.web.ListerCollaborateursControlle</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditerCollaborateursControlle</servlet-name>
		<servlet-class>dev.sgp.web.EditerCollaborateursControlle</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListerCollaborateursControlle</servlet-name>
		<url-pattern>/collaborateurs/lister</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>EditerCollaborateursControlle</servlet-name>
		<url-pattern>/collaborateurs/editer</url-pattern>
	</servlet-mapping>
</web-app>
